function sortArr(arr) {
  arr.sort(function (a, b) {
    if (a.length !== b.length) {
      return a.length - b.length;
    } else {
      return a.toLowerCase().localeCompare(b.toLowerCase());
    }
  });
  for(let i = 0; i < arr.length; i++) {
    console.log(arr[i]);
  }
  
}

sortArr(["test", "Deny", "omen", "Default"]);
